<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="DefaultWrap">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
  </style>
  
  <style name="DefaultFill">
  	<item name="android:layout_width">fill_parent</item>
  	<item name="android:layout_height">fill_parent</item>
  </style>
  
  <style name="DefaultWidthFill">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
  </style>
   
  <style name="DefaultHeightFill">
    <item name="android:layout_width">wrap_content</item>
  	<item name="android:layout_height">fill_parent</item>
  </style>
  
  <style name="DefaultLinearWrap" parent="DefaultWrap">
  	<item name="android:orientation">vertical</item>
  	<item name="android:padding">7dp</item>
  </style>
  
  <style name="DefaultLinearFill" parent="DefaultFill">
  	<item name="android:orientation">vertical</item>
  </style>
  
  <style name="DefaultRelativeWrap" parent="DefaultWrap">
  	<item name="android:padding">7dp</item>
  </style>  
  
  <style name="DefaultRelativeFill" parent="DefaultFill">
  	<item name="android:padding">7dp</item>
  </style>  
  
  <style name="Cell" parent="DefaultWidthFill">
  	<item name="android:padding">7dp</item>
  </style>
  
  <!-- everywhere used style -->
  <style name="SmallGrayText" parent="DefaultWrap">
  	<item name="android:textSize">@dimen/text_small</item>
  	<item name="android:textColor">@color/GRAY</item>
  </style>

  <style name="SmallOrangeText" parent="DefaultWrap">
  	<item name="android:textSize">@dimen/text_small</item>
  	<item name="android:textColor">@color/DARK_ORANGE</item>
  </style>
  
  <style name="ETCButton">
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">40dp</item>
  	<item name="android:padding">7dp</item>
  	<item name="android:textSize">16dp</item>
  	<item name="android:gravity">left|center</item>
  	<item name="android:drawableRight">@drawable/img_moresign01</item>
  </style>
  
  <style name="SettingButton" parent="ETCButton">
  	<item name="android:background">@drawable/btn_rectangle</item>
  	<item name="android:layout_width">fill_parent</item>
  	<item name="android:layout_height">wrap_content</item>
  </style>
  
  <!-- title_container.xml -->
  <style name="title_container_Default" parent="DefaultHeightFill">
  	<item name="android:gravity">center</item>
  </style>
  
  <!-- activity_store_main.xml -->
  <style name="store_main_Cell" parent="Cell">
  	<item name="android:layout_weight">1</item>
  </style>
  
  <style name="store_main_Name">
  	<item name="android:layout_width">170dp</item>
  	<item name="android:layout_height">30dp</item>
	<item name="android:textSize">23dp</item>
  	<item name="android:textColor">#000</item>
  </style>

  <style name="store_main_InfoButton">
  	<item name="android:layout_width">30dp</item>
  	<item name="android:layout_height">30dp</item>
  	<item name="android:background">@drawable/btn_info</item>
  	<item name="android:layout_marginLeft">5dp</item>
  </style>
  
  <style name="store_main_Thumnail">
  	<item name="android:background">@drawable/thumnail_bg</item>
	<item name="android:layout_width">100dp</item>
	<item name="android:layout_height">90dp</item>
	<item name="android:layout_marginRight">6dp</item>
	<item name="android:layout_marginBottom">6dp</item>
	<item name="android:scaleType">centerCrop</item>
  </style>
  
  <style name="store_main_LikeButton">
  	<item name="android:background">@drawable/like_bg</item>
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">30dp</item>
  	<item name="android:layout_marginLeft">3dp</item>
  	<item name="android:textSize">15dp</item>
  	<item name="android:textColor">#f00</item>
  </style>

  <style name="store_main_Tel" parent="DefaultWidthFill">
	<item name="android:textSize">15dp</item>
	<item name="android:textColor">#aaa</item>
  </style>

  <style name="store_main_Address" parent="DefaultWidthFill">
  	<item name="android:textSize">15dp</item>
  	<item name="android:textColor">#aaa</item>
  </style>

  <style name="store_main_Tag" parent="DefaultWidthFill">
  	<item name="android:textSize">15dp</item>
  	<item name="android:textColor">#aaa</item>
  </style>
  
  <style name="store_main_Food" parent="DefaultWidthFill">
  	<item name="android:textSize">15dp</item>
  	<item name="android:textColor">#aaa</item>
  </style>
  
  <style name="store_main_ScrapButton">
  	<item name="android:layout_width">100dp</item>
  	<item name="android:layout_height">25dp</item>
  	<item name="android:text">@string/store_main_bookmark</item>
  	<item name="android:textSize">12dp</item>
  	<item name="android:textColor">#fff</item>
  </style>
  
  <style name="store_main_StoreCellButton">
<!--   	<item name="android:background">@drawable/btn_bg_h31px</item> -->
  	<item name="android:background">@drawable/selector</item>
  	<item name="android:layout_width">60dp</item>
  	<item name="android:layout_height">25dp</item>
  	<item name="android:layout_marginLeft">3dp</item>
  	<item name="android:textSize">11dp</item>
  </style>
  
  <style name="store_main_Cover">
  	<item name="android:layout_width">fill_parent</item>
  	<item name="android:layout_height">60dp</item>
  	<item name="android:textColor">#000</item>
	<item name="android:textSize">16dp</item>
	<item name="android:textStyle">bold</item>
  </style>
  
  <style name="store_main_MenuAndTag">
  	<item name="android:layout_width">wrap_content</item>
  	<item name="android:layout_height">wrap_content</item>
  	<item name="android:padding">1dp</item>
  	<item name="android:textSize">12dp</item>
  	<item name="android:singleLine">true</item>
  </style>
  
  <style name="store_main_User">
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">wrap_content</item>
  	<item name="android:drawableLeft">@drawable/bullet_point_01</item>
  	<item name="android:drawablePadding">3dp</item>
  	<item name="android:textColor">#777</item>
  	<item name="android:textSize">11dp</item>
  </style>
  <style name="store_main_ViewButton">
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">40dp</item>
  	<item name="android:textSize">16dp</item>
  </style>
  
  <!-- header_post.xml -->
  <style name="header_post_Preview" parent="DefaultWrap">
  	<item name="android:background">@drawable/img_border</item>
  	<item name="android:adjustViewBounds">true</item>
  	<item name="android:padding">3dp</item>
  	<item name="android:visibility">gone</item>
  </style>
  
  <!-- cell_user.xml --> 
  <style name="user_cell_Thumnail">
  	<item name="android:background">@drawable/thumnail_bg</item>
	<item name="android:layout_width">60dp</item>
	<item name="android:layout_height">60dp</item>
	<item name="android:layout_marginRight">7dp</item>
	<item name="android:layout_marginBottom">7dp</item>
	<item name="android:scaleType">centerCrop</item>
  </style>

  <style name="user_cell_Mileage" parent="DefaultWrap">
  	<item name="android:textSize">11dp</item>
  </style>
  
  <style name="user_cell_Grade" parent="user_cell_Mileage">
  	<item name="android:drawablePadding">3dp</item>
  	<item name="android:textColor">@color/SKYBLUE</item>
  </style>
  
  <style name="user_cell_PointText" parent="user_cell_Mileage">
  	<item name="android:drawablePadding">10dp</item>
  	<item name="android:drawableLeft">@drawable/img_classify</item>
    <item name="android:layout_marginLeft">10dp</item>
  </style>
  
  <style name="user_cell_Point" parent="user_cell_Mileage">
  	<item name="android:textColor">@color/DARK_ORANGE</item>
  </style>
  
  <style name="user_cell_Title" parent="DefaultWrap">
  	<item name="android:textSize">18dp</item>
  	<item name="android:textStyle">bold</item>
  	<item name="android:textColor">#333333</item>
  </style>
  
  <style name="user_cell_Intro" parent="DefaultWrap">
  </style>
  <!-- activity_user_main.xml -->  
  <style name="user_main_Cell" parent="Cell">
  </style>
  
  <style name="user_main_FollowButton" parent="DefaultWidthFill">
  	<item name="android:layout_marginTop">7dp</item>
  	<item name="android:textSize">16dp</item>
  	<item name="android:textStyle">bold</item>
  </style>

  <style name="user_main_FollowingYou" parent="DefaultWidthFill">
    <item name="android:gravity">center</item>
  </style>

  <style name="user_main_InfoText" parent="DefaultWidthFill">
  	<item name="android:drawableLeft">@drawable/bullet_point_01</item>
	<item name="android:drawablePadding">3dp</item>
	<item name="android:textSize">11dp</item>
	<item name="android:gravity">left</item>
  </style>
  
  <style name="user_main_BlogButton" parent="DefaultWrap">
	<item name="android:textSize">11dp</item>
	<item name="android:textStyle">bold</item>
  </style>
  
  <style name="user_main_MenuAndTagImage">
  	<item name="android:layout_width">25dp</item>
  	<item name="android:layout_height">wrap_content</item>
  	<item name="android:padding">1dp</item>
  </style>
  
  <style name="user_main_MenuAndTag">
  	<item name="android:layout_width">wrap_content</item>
  	<item name="android:layout_height">wrap_content</item>
  	<item name="android:padding">1dp</item>
  	<item name="android:textSize">15dp</item>
  	<item name="android:textStyle">bold</item>
  </style>
  
  <style name="uesr_main_User">
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">wrap_content</item>
  	<item name="android:drawableLeft">@drawable/bullet_point_01</item>
  	<item name="android:drawablePadding">3dp</item>
  	<item name="android:textColor">#777</item>
  	<item name="android:textSize">11dp</item>
  </style>
  <style name="user_main_ViewButton">
  	<item name="android:layout_width">150dp</item>
  	<item name="android:layout_height">40dp</item>
  	<item name="android:textSize">16dp</item>
  	<item name="android:textStyle">bold</item>
  </style>

  <!-- activity_grid_gallery.xml -->
  <style name="grid_gallery_GridItem" parent="DefaultFill">
  	<item name="android:numColumns">auto_fit</item>
  	<item name="android:verticalSpacing">5dp</item>
  	<item name="android:horizontalSpacing">5dp</item>
  	<item name="android:columnWidth">96dp</item>
  	<item name="android:stretchMode">columnWidth</item>
  	<item name="android:gravity">center</item>
  </style>

  <!-- activity_main_map.xml -->
  <style name="main_map_searchBox" parent="DefaultWrap">
    <item name="android:layout_width">120dp</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="main_map_commonButton" parent="DefaultWrap">
    <item name="android:layout_width">50dp</item>
    <item name="android:singleLine">true</item>
    <item name="android:textSize">10dp</item>
  </style>

  <!-- activity_get_map_position.xml -->
  <style name="get_map_position_addressText" parent="DefaultWidthFill">
    <item name="android:background">@color/address_text_bg</item>
    <item name="android:gravity">center</item>
    <item name="android:padding">5dp</item>
  </style>

  <style name="get_map_position_currentPositionBtn" parent="DefaultWrap">
    <item name="android:layout_width">50dp</item>
    <item name="android:singleLine">true</item>
    <item name="android:textSize">10dp</item>
  </style>

  <style name="get_map_position_registSubmitBtn" parent="DefaultWrap">
    <item name="android:singleLine">true</item>
    <item name="android:textSize">12dp</item>
  </style>

  <!-- popup_overlay.xml -->
  <style name="PopupOverlayBallon" parent="DefaultWrap">
    <item name="android:paddingLeft">5dp</item>
    <item name="android:paddingTop">0dp</item>
    <item name="android:paddingRight">23dp</item>
    <item name="android:paddingBottom">9dp</item>
    <item name="android:background">@drawable/map_popup</item>
    <item name="android:scaleType">centerCrop</item>
  </style>

  <style name="PopupOverlayTitle" parent="DefaultWrap">
    <item name="android:maxWidth">150dp</item>
    <item name="android:textSize">14dp</item>
    <item name="android:textColor">@color/BRIGHT_ORANGE</item>
    <item name="android:singleLine">true</item>
  </style>

  <style name="PopupOverlayContent" parent="PopupOverlayTitle">
    <item name="android:textSize">12dp</item>
    <item name="android:textColor">@color/WHITE</item>
  </style>

  <!-- indicator_round common attributes -->
  <style name="IndicatorRound" parent="DefaultWidthFill">
    <item name="android:padding">7dp</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="IndicatorRoundText" parent="DefaultWrap">
    <item name="android:layout_gravity">center</item>
    <item name="android:textColor">@color/minor_tab_text_default</item>
  </style>
    
  <!-- indicator_round_head.xml -->
  <style name="IndicatorRoundHead" parent="IndicatorRound">
    <item name="android:background">@drawable/btn_left_corner</item>
  </style>

  <!-- indicator_round_left.xml -->
  <style name="IndicatorRoundLeft" parent="IndicatorRound">
    <item name="android:background">@drawable/indicator_left_corner</item>
  </style>
  
  <!-- indicator_round_center.xml -->
  <style name="IndicatorRoundCenter" parent="IndicatorRound">
    <item name="android:background">@drawable/indicator_center_rect</item>
  </style>

  <!-- indicator_round_right.xml -->
  <style name="IndicatorRoundRight" parent="IndicatorRound">
    <item name="android:background">@drawable/indicator_right_corner</item>
  </style>
  
  <!-- indicator_main.xml -->
  <style name="IndicatorMain" parent="DefaultWidthFill">
    <item name="android:background">@drawable/indicator_main</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="IndicatorMainIcon" parent="DefaultWrap">
  </style>

  <style name="IndicatorMainText" parent="DefaultWrap">
    <item name="android:textColor">@drawable/indicator_main_text</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="IndicatorMainBottomBar" parent="DefaultWidthFill">
    <item name="android:layout_height">1dp</item>
    <item name="android:background">@drawable/indicator_main_bottom_bar</item>
  </style>

  <style name="IndicatorMainRightBar" parent="DefaultHeightFill">
    <item name="android:layout_width">1dp</item>
    <item name="android:background">@color/main_tab_right_bar</item>
  </style>

  <!-- activity_post_tab.xml -->
  <style name="post_tab_TabBar" parent="DefaultWidthFill">
    <item name="android:padding">6dp</item>
    <item name="android:background">@color/main_tab_focused</item>
  </style>

  <style name="post_tab_DivisionBar" parent="DefaultFill">
    <item name="android:layout_height">2dp</item>
    <item name="android:background">@color/post_tab_division_bar</item>
  </style>
  
  <!-- Thumnail -->
  <style name="ThumnailWrapper">
    <item name="android:layout_width">@dimen/thumnail_size</item>
    <item name="android:layout_height">@dimen/thumnail_size</item>
    <item name="android:background">@drawable/thumnail_bg</item>
  </style>
  
  <style name="Thumnail">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">fill_parent</item>
    <item name="android:scaleType">centerCrop</item>
  </style>
  
  <!-- Post List Row -->
  <style name="row_post_PostRowWrapper" parent="DefaultWidthFill">
  	<item name="android:paddingLeft">@dimen/default_distance</item>
  	<item name="android:paddingRight">@dimen/default_distance</item>
  	<item name="android:paddingTop">@dimen/default_distance_half</item>
  	<item name="android:paddingBottom">@dimen/default_distance_half</item>
  </style>
  
  <style name="row_post_PostWrapper" parent="DefaultWidthFill">
  	<item name="android:background">@drawable/post_bg</item>
  	<item name="android:orientation">vertical</item>
  	<item name="android:layout_marginLeft">@dimen/row_post_distance</item>
  </style>

  <style name="row_post_Name" parent="DefaultWrap">
	<item name="android:textColor">@color/DARK_ORANGE</item>
  </style>

  <style name="row_post_At" parent="DefaultWrap">
  	<item name="android:textColor">@color/GRAY</item>  
  	<item name="android:text">@string/at</item>  
  </style>
  
  <style name="row_post_PostContent" parent="DefaultWrap">
  	<item name="android:autoLink">web</item>
  	<item name="android:textColor">@color/BLACK</item>
  </style>
  
  <style name="row_post_Tags" parent="SmallGrayText">
   	<item name="android:drawableLeft">@drawable/img_tag</item>
  	<item name="android:drawablePadding">@dimen/default_distance_half</item>
  </style>
  
  <style name="row_post_CreatedAt" parent="SmallGrayText">
    <item name="android:paddingRight">@dimen/default_distance</item>
  </style>

  <style name="row_post_Comment" parent="SmallOrangeText">
    <item name="android:text">@string/default_string_comment</item>
  </style>
  
  <style name="row_post_Count" parent="SmallGrayText">
  	<item name="android:paddingLeft">@dimen/default_distance</item>
  	<item name="android:paddingRight">@dimen/default_distance</item>
  </style>
  
  <style name="row_post_Like" parent="SmallOrangeText">
   	<item name="android:drawableLeft">@drawable/img_like_cnt</item>
  </style>
  
</resources>